<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Cake.Wallpaper.Manager.GUI.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Cake.Wallpaper.Manager.GUI.Views.FranchiseSelectDialogueWindow"
        Title="FranchiseSelectDialogueWindow">
    <Design.DataContext>
        <viewModels:FranchiseSelectDialogueWindowViewModel />
    </Design.DataContext>
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Command="{Binding DoneCommand}" Header="Done"></MenuItem>
            <MenuItem Header="Cancel"></MenuItem>
        </Menu>
        <TextBox Watermark="Search..." DockPanel.Dock="Top"></TextBox>
        <!--<ListBox DockPanel.Dock="Left" Items="{Binding DbFranchises}"></ListBox>-->
        <ListBox
            DockPanel.Dock="Right" Items="{Binding DbFranchises}">
        </ListBox>
    </DockPanel>
</Window>